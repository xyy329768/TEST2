#w5{
	width: 35px;
	height: 100%;
	position: fixed;
	top: 0;
	right: 0;
	z-index: 999;
	background-color: rgb(92,92,92);
}
#w5 .w5-aside{
	position: absolute;
	bottom: 35px;
	right: 0;
	width: 35px;
	color: #999999;
	font-size: 12px;
	z-index: 999;
}
#w5 ul{
	list-style: none;
}
.w5-aside ul li{
	position: relative;
	text-align: center;
	height: 38px;
	font-size: 12px;
}
.w5-aside ul li b{
	position: absolute;
	top: 50%;
	left: -1px;
	margin-top: -6px;
	width: 5px;
	height: 9px;
	display: inline-block;
}
.w5-aside ul li a{
	display: inline-block;
	width: 100%;
	background: #5c5c5c;
	padding: 9px 0;
	height: 20px;
	color: #999;
	cursor: pointer;
	overflow: hidden;
	z-index: 2;
	position: relative;
}
.w5-aside i{
	background: url(../img/ui.png) -47px -464px;
	display: inline-block;
	width: 20px;
	height: 20px;
	font-style: normal;
}
.w5-aside ul li.w5-aside1 i{
	background-position: -212px -473px;
}
.w5-aside ul li.w5-aside2 i{
	background-position: -212px -493px;
}
.w5-aside ul li.w5-aside3 i{
	color: #c00;
	background-position: -212px -513px;
}
.w5-aside ul li.w5-aside4 i{
	background-position: -212px -533px;
}
#w5 .gome-tab{
	position: relative;
	width: 35px;
	height: 100%;
	top: 0;
	right: 0;
	background: #5c5c5c;
	z-index: 998;
}
.gome-tab{
	cursor: pointer;
}
.gome-tab ul{
	position: absolute;
	right: 0;
}
.gome-tab .gome-tab1 b{
	background: url(../img/ui.png) no-repeat -18px -1025px;
	display: inline-block;
	width: 28px;
	height: 18px;
	margin-top: 154px;
	margin-left: 15px;
}
.gome-tab .gome-tab1 i{
	background: url(../img/uiline.png) no-repeat;
	display: inline-block;
	width: 27px;
	height: 1px;
	position: absolute;
	top: 180px;
	right: 4px;
}
.gome-tab .gome-tab2 i{
	background: url(../img/uiline.png) no-repeat;
	display: inline-block;
	width: 27px;
	height: 1px;
	margin-top: 115px;
	position: absolute;
	right: 4px;
}
.gome-tab .gome-tab3 i{
	background: url(../img/2017-10-21_161751.png) no-repeat;
	display: inline-block;
	width: 27px;
	height: 1px;
	margin-top: 160px;
	position: absolute;
	right: 4px;
}
.gome-tab2 img{
	display: inline-block;
	position: absolute;
	right: 4px;
	margin-top: 12px;
}
.gome-tab2 a{
	font-size: 12px;
	color: #FFFFFF;
	font-family: "微软雅黑";
	text-decoration: none;
	display: inline-block;
	width: 12px;
	margin-top: 55px;
	position: absolute;
	right: 12px;
}
.gome-tab3 img{
	margin-top: 125px;
	position: absolute;
	right: 5px;
}