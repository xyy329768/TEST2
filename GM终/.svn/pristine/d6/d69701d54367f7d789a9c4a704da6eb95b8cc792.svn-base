}

.dorpdown-layer {
	display: none;
	position: absolute;
}

.hide {
	display: none;
}


#shortcut {
	width: 100%;
	height: 30px;
	line-height: 30px;
	background: #F1F1F1;
}

.fl {
	float: left;
}

#shortcut .w {
